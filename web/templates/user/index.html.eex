<div class="col-md-12">
  <h3>Users</h3>

  <ul>
    <%= for user <- @users do %>
      <li>
        <a href="<%= Router.user_path(id: user.id) %>">
          <%= user.content %>
        </a>
      </li>
    <% end %>
  </ul>

  <a href="<%= Router.new_user_path%>" class="btn btn-primary">
    New User
  </a>
</div>
